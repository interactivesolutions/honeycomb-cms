{
  "name": "interactivesolutions/honeycomb-cms",
  "description": "Main HoneyComb package to install all core functionality of the CMS",
  "keywords": [
    "honeycombcms",
    "honeycomb",
    "cms",
    "package",
    "cmspackage",
    "cms functionality",
    "core",
    "acl",
    "scripts",
    "resources",
    "languages",
    "regions",
    "slugs",
    "url-shortener"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mantas GelÄius",
      "email": "mantas.gelcius@gmail.com"
    },
    {
      "name": "Liutauras Kavaliauskas",
      "email": "liutauraskav@gmail.com"
    },
    {
      "name": "Jevgenij Dmitrijev",
      "email": "jd@interactivesolutions.lt"
    },
    {
      "name": "Vytautas Rimeikis",
      "email": "vytautas.rimeikis@gmail.com"
    }
  ],
  "repositories": [],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "5.3.*",
    "interactivesolutions/honeycomb-acl": "^1.0.0",
    "interactivesolutions/honeycomb-apps": "^1.0.0",
    "interactivesolutions/honeycomb-core": "^1.0.0",
    "interactivesolutions/honeycomb-core-ui": "~1.0.0",
    "interactivesolutions/honeycomb-galleries": "~1.0.0",
    "interactivesolutions/honeycomb-languages": "~1.0.0",
    "interactivesolutions/honeycomb-regions": "~1.0.0",
    "interactivesolutions/honeycomb-pages": "~1.0.0",
    "interactivesolutions/honeycomb-resources": "~1.0.0",
    "interactivesolutions/honeycomb-scripts": "^1.0.0",
    "interactivesolutions/honeycomb-slugs": "~1.0.0",
    "interactivesolutions/honeycomb-url-shortener": "~1.0.0",
    "interactivesolutions/honeycomb-translations": "~1.0.0",
    "interactivesolutions/honeycomb-menu": "~1.0.0"
  },
  "require-dev": {
    "brianium/paratest": "^0.14",
    "mockery/mockery": "^0.9",
    "orchestra/testbench-browser-kit": "^3.3",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "InteractiveSolutions\\HoneycombCms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "paratest -p4 --colors"
    ]
  },
  "config": {
    "secure-http": false,
    "platform": {
      "php": "7.1"
    }
  }
}